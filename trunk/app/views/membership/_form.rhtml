<%= error_messages_for 'edit' %>

<!--[form:membership]-->
<div class="row"> 
<label for="institution_id" class="label"><%= get_label('institution_id') %> <span class="required">*</span></label> 
<div id="institution_id">
<%= select_without_universities('edit', Institution,  1, 1) %> 
</div>
</div>

<div class="row"> 
<label for="descr" class="label"><%= get_label('descr') %></label> 
<%= text_field 'edit', 'descr', 'size' => 30, 'maxsize'=> 40, 'tabindex'=> 2, 'id' => 'descr' %>
</div>

<div class="row"> 
<label for="startyear" class="label"><%= get_label('startyear') %></label> 
<%= year_select('edit', 'startyear', {:tabindex => 3, :required => 0 }) %> 
</div>

<div class="row"> 
<label for="endyear" class="label"><%= get_label('endyear') %></label> 
<%= year_select('edit', 'endyear', {:tabindex => 4, :required => 0 }) %> 
</div>


