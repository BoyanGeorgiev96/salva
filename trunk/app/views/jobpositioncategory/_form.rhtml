<div class="row"> 
<label for="name" class="label"><%= get_label('name') %> <span class="required">*</span></label> 
<%= text_field 'edit', 'name', 'size' => 30, 'maxsize'=> 40, 'tabindex'=> 1, 'id' => 'name', 'z:required' => 'true', 'z:message' => 'Este campo es requerido' %>
</div>

<div class="row"> 
<label for="jobpositiontype_id" class="label"><%= get_label('jobpositiontype_id') %> <span class="required">*</span></label> 
<div id="jobpositiontype_id">
<%= table_select('edit', Jobpositiontype, {:tabindex => 2, :required => 1 }) %> 
<%= quickpost('jobpositiontype') %> 
</div>
</div>

<div class="row"> 
<label for="jobpositionlevel_id" class="label"><%= get_label('jobpositionlevel_id') %> <span class="required">*</span></label> 
<div id="jobpositionlevel_id">
<%= table_select('edit', Jobpositionlevel, {:tabindex => 3, :required => 1 }) %> 
<%= quickpost('jobpositionlevel') %> 
</div>
</div>

<div class="row"> 
<label for="administrative_key" class="label"><%= get_label('administrative_key') %></label> 
<%= text_field 'edit', 'administrative_key', 'size' => 30, 'maxsize'=> 40, 'tabindex'=> 4, 'id' => 'administrative_key' %>
</div>


