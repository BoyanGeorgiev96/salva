<%= table_select('edit', Country, 13, 1) %>
</div>
</div>

<div class="row"> 
<label for="publisher_id" class="label"><%= get_label('publisher_id') %> <span class="required">*</span></label> 
<div id="publisher_id">
<%= table_select('edit', Publisher, 12, 1) %>
</div>
<%= submit_for_stack('publisher') %>
