<%= text_field 'edit', 'impactfactor', 'size' => 30, 'maxlength'=> 40, 'tabindex'=> 7, 'id' => 'impactfactor' %>
</div>

<div class="row"> 
<label for="immediacy" class="label"><%= get_label('immediacy') %></label> 
<%= text_field 'edit', 'immediacy', 'size' => 30, 'maxlength'=> 40, 'tabindex'=> 8, 'id' => 'immediacy' %>
</div>

<div class="row"> 
<label for="dateupdate" class="label"><%= get_label('dateupdate') %></label> 
<%= text_field 'edit', 'dateupdate', 'size' => 30, 'maxlength'=> 40, 'tabindex'=> 9, 'id' => 'dateupdate' %>
</div>

<div class="row"> 
<label for="journaltype_id" class="label"><%= get_label('journaltype_id') %> <span class="required">*</span></label> 
<div id="journaltype_id">
<%= table_select('edit', Journaltype, 10, 1) %>
</div>
</div>

