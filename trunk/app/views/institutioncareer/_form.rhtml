<div class="row"> 
<label for="abbrev" class="label"><%= get_label('abbrev') %></label> 
<%= text_field 'edit', 'abbrev', 'size' => 30, 'maxsize'=> 40, 'tabindex'=> 1, 'id' => 'abbrev' %>
</div>

