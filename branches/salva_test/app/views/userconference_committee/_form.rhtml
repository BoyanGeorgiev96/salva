<label for="roleinconference_id" class="label"><%= get_label('roleinconference_id') %> <span class="required">*</span></label> 
<div id="roleinconference_id">
<%= select_as_tree('edit', Roleinconference, %w(name), 1, 1, :conditions => "name != 'Asistente'") %>
</div>
</div>


