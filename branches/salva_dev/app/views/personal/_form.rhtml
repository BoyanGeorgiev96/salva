<label for="birthcity"><%= salva_column('birthcity') %></label> 
<%= text_field 'edit', 'birthcity' %>
</div>

<div class="row"> 
<label for="dateofbirth"><%= salva_column('dateofbirth') %></label> 
<%= text_field 'edit', 'dateofbirth' %>
</div>

<div class="row"> 
<label for="lastname1"><%= salva_column('lastname1') %></label> 
<%= text_field 'edit', 'lastname1' %>
</div>

<div class="row"> 
<label for="firstname"><%= salva_column('firstname') %></label> 
<%= text_field 'edit', 'firstname' %>
</div>

