<label for="sex"><%= salva_column('sex') %></label> 
<%= text_field 'edit', 'sex' %>
</div>

<div class="row"> 
<label for="maritalstatu_id"><%= salva_column('maritalstatu_id') %></label> 
<div id="Maritalstatu">
<%= table_select('edit', Maritalstatu) %> 
